  </ns0:Value>
  <ns0:Value id="xccdf_preupg_value_state_migrate" operator="equals" type="string">
    <ns0:value>0</ns0:value>
  </ns0:Value>
   <ns0:Group id="xccdf_preupg_group_dummy_preupg" selected="true">
